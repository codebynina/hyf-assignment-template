let peterHouseWidth = 8;
let peterHouseDepth = 10;
let peterHouseHieght = 10;
let peterGardenSize = 100;
let peterHousePrize = 2500000;

let juliaHouseWidth = 5;
let juliaHouseDepth = 11;
let juliaHouseHeight = 8;
let juliaGardenSize = 70;
let juliaHousePrize = 1000000;

let peterVolume = peterHouseWidth * peterHouseDepth * peterHouseHieght;
let juliaVolume = juliaHouseWidth * juliaHouseDepth * juliaHouseHeight;

let peterEstimatedHousePrize = peterVolume * 2.5 * 1000 + peterGardenSize * 300;
let juliaEstimatedHousePrize = juliaVolume * 2.5 * 1000 + juliaGardenSize * 300;



if (peterHousePrize>peterEstimatedHousePrize) {
    console.log ("Peter is paying too much for the house.")

    } else {
        console.log ("Peter is getting a good deal.");
    }


if (juliaHousePrize>juliaEstimatedHousePrize) {
    console.log ("Julia is paying too much for the house.")

} else {
    console.log("Julia is getting a good deal.");
}